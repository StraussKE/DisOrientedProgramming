@{
}

<h1>@ViewBag.Topic</h1>

@for (int i = 0; i < ViewBag.Posts.Count; i++)
{
<a asp-action="ViewThread" asp-controller="Forum" asp-route-startPost="@ViewBag.Posts[i].ForumPostId" class="Normal-link"><h3>@ViewBag.Posts[i].PostTitle</h3></a>
}
